# Apache Configuration File

<IfModule mod_rewrite.c>
    RewriteEngine on
#   RewriteBase /

    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d

    RewriteRule ^(.*)$ index.php/$1
</IfModule>